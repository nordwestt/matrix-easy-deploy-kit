version: 1

formatters:
  precise:
    format: "%(asctime)s - %(name)s - %(lineno)d - %(levelname)s - %(request)s - %(message)s"

handlers:
  console:
    class: logging.StreamHandler
    formatter: precise

loggers:
  synapse.storage.SQL:
    # Uncomment the line below to log all SQL queries â€” very verbose.
    # level: DEBUG
    level: WARNING

root:
  level: WARNING
  handlers: [console]

disable_existing_loggers: false
